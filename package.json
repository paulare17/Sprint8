{
  "name": "sprint8",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:server": "ts-node-dev --respawn --transpile-only api/index.ts",
    "dev:fullstack": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "NODE_ENV=production tsc -b && vite build && tsc -p api/tsconfig.json",
    "build:client": "tsc -b && vite build",
    "build:server": "NODE_ENV=production tsc -p api/tsconfig.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "NODE_ENV=production node api/dist/index.js",
    "start:client": "vite preview",
    "vercel-build": "NODE_ENV=production npm run build"
  },
  "dependencies": {
